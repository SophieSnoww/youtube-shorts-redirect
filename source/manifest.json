{
	"name": "YT Shorts Redirecter",
	"version": "1.0.0",
	"description": "Redirects youtube shorts links to normal youtube links",
	"homepage_url": "https://github.com/sophiesnoww/youtube-shorts-redirect",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"permissions": [
        "tabs"
	],
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	}
}
